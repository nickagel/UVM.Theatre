@model IEnumerable<UVM.Theatre.Domain.Models.Show>

@{
  ViewBag.Title = "Arhives";
  var n = 0;
}

<ul id="archives">
@while (n++ < 6)
{
  
     foreach (var show in @Model)
     {
       <li class="archive">
           <a href='@($"/Theatre/ArhivedShow/{@show.ShowId}")'><img class="border" src="@show.Image"/></a>
         
       </li>
     }
}
</ul>